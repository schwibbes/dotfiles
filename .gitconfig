[user]
	name = schwibbes2
	email = schwibbes2@gmail.com
[alias]
	st = status -sb
	amend = commit --amend --no-edit
	co = checkout
	lg = log --graph --decorate -n 20 --pretty=oneline --abbrev-commit
	ls = ls-files
	ignored = ls-files -o -i --exclude-standard
	wip = !git add . && git commit -m "WIP"
[color]
	ui = true
[push]
	default = simple
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
